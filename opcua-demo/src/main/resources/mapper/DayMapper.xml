<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rookie.opcua.mapper.DayMapper">
    <select id="findDay" resultType="com.rookie.opcua.entity.Day">
        select * from day where id = '1'
    </select>
    <update id="updateTime" parameterType="String">
        update day set day = #{time}
    </update>
</mapper>